public Boolean isPalindrome(String str){
//code here
    str = str.toLowerCase();
    str = str.replaceAll('[^a-z]', '');
    
    Integer max = str.length() - 1;
    for(Integer i = 0 ; i<= str.length() / 2;i++){
        if(str.charAt(i) != str.charAt(max - i)) return false;
        
    }
    
    return true;
}