public List<apxio__Student__c> selectUnreachableStudents() {
    // code here
    List<apxio__Student__c> activeStudents = [SELECT ID,Name,apxio__Email__c,apxio__Registration_Number__c  
                    From apxio__Student__c 
                    WHERE apxio__Active__c = true];
    List<apxio__Student__c> unreachableStudents = new List<apxio__Student__c>();
    for(apxio__Student__c student : activeStudents){
        if(String.isBlank(student.apxio__Email__c) 
          && String.isBlank(student.apxio__Registration_Number__c)){
            unreachableStudents.add(student);
          }
    }

    return unreachableStudents;
}